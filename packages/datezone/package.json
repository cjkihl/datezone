{
  "name": "datezone",
  "version": "1.0.1",
  "description": "A lightweight and comprehensive date and timezone utility library for JavaScript.",
  "keywords": [
    "date",
    "timezone",
    "utilities",
    "javascript",
    "typescript",
    "intl",
    "date-fns",
    "performance",
    "tree-shakeable"
  ],
  "homepage": "https://github.com/cjkihl/datezone#readme",
  "bugs": {
    "url": "https://github.com/cjkihl/datezone/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cjkihl/datezone.git"
  },
  "license": "MIT",
  "author": "CJ Kihl",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "default": "./dist/output/index.pub.js",
      "types": "./dist/types/index.pub.d.ts"
    },
    "./compare": {
      "default": "./dist/output/compare.pub.js",
      "types": "./dist/types/compare.pub.d.ts"
    },
    "./constants": {
      "default": "./dist/output/constants.pub.js",
      "types": "./dist/types/constants.pub.d.ts"
    },
    "./day": {
      "default": "./dist/output/day.pub.js",
      "types": "./dist/types/day.pub.d.ts"
    },
    "./hour": {
      "default": "./dist/output/hour.pub.js",
      "types": "./dist/types/hour.pub.d.ts"
    },
    "./minute": {
      "default": "./dist/output/minute.pub.js",
      "types": "./dist/types/minute.pub.d.ts"
    },
    "./month": {
      "default": "./dist/output/month.pub.js",
      "types": "./dist/types/month.pub.d.ts"
    },
    "./offset": {
      "default": "./dist/output/offset.pub.js",
      "types": "./dist/types/offset.pub.d.ts"
    },
    "./ordinal": {
      "default": "./dist/output/ordinal.pub.js",
      "types": "./dist/types/ordinal.pub.d.ts"
    },
    "./second": {
      "default": "./dist/output/second.pub.js",
      "types": "./dist/types/second.pub.d.ts"
    },
    "./timezone": {
      "default": "./dist/output/timezone.pub.js",
      "types": "./dist/types/timezone.pub.d.ts"
    },
    "./walltime": {
      "default": "./dist/output/walltime.pub.js",
      "types": "./dist/types/walltime.pub.d.ts"
    },
    "./week": {
      "default": "./dist/output/week.pub.js",
      "types": "./dist/types/week.pub.d.ts"
    },
    "./year": {
      "default": "./dist/output/year.pub.js",
      "types": "./dist/types/year.pub.d.ts"
    }
  },
  "files": [
    "dist/**/*",
    "README.md",
    "CHANGELOG.md",
    "CONTRIBUTING.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc && create-exports",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@cjkihl/tsconfig": "^1.0.2"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
