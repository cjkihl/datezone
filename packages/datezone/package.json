{
  "name": "datezone",
  "version": "1.0.1",
  "description": "A lightweight and comprehensive date and timezone utility library for JavaScript.",
  "keywords": [
    "date",
    "timezone",
    "utilities",
    "javascript",
    "typescript",
    "intl",
    "date-fns",
    "performance",
    "tree-shakeable"
  ],
  "homepage": "https://github.com/cjkihl/datezone#readme",
  "bugs": {
    "url": "https://github.com/cjkihl/datezone/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cjkihl/datezone.git"
  },
  "license": "MIT",
  "author": "CJ Kihl",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "default": "./dist/output/index.pub.js",
      "types": "./dist/types/index.pub.d.ts"
    }
  },
  "main": "./dist/output/index.pub.js",
  "types": "./dist/types/index.pub.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "CHANGELOG.md",
    "CONTRIBUTING.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc && create-exports",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@cjkihl/tsconfig": "^1.0.1"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
