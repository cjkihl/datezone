{
	"name": "datezone",
	"sideEffects": false,
	"version": "0.5.1",
	"description": "A lightweight and comprehensive date and timezone utility library for JavaScript.",
	"main": "./.dist/output/index.pub.js",
	"types": "./.dist/types/index.pub.d.ts",
	"files": [
		".dist/**/*",
		"README.md",
		"LICENSE"
	],
	"keywords": [
		"date",
		"timezone",
		"utilities",
		"javascript",
		"typescript",
		"intl",
		"date-fns",
		"performance",
		"tree-shakeable"
	],
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/cjkihl/datezone.git"
	},
	"bugs": {
		"url": "https://github.com/cjkihl/datezone/issues"
	},
	"homepage": "https://github.com/cjkihl/datezone#readme",
	"author": "CJ Kihl",
	"exports": {
		".": {
			"types": "./.dist/types/index.pub.d.ts",
			"default": "./.dist/output/index.pub.js"
		}
	},
	"type": "module",
	"engines": {
		"node": ">=16.0.0"
	},
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"build": "tsc && create-exports",
		"publish": "bun publish",
		"ts": "tsc --noEmit"
	},
	"dependencies": {
		"@cj/config": "workspace:*"
	},
	"devDependencies": {
		"@cj/mono": "workspace:*"
	}
}
